<ol>
  {% for link in section.settings.menu.links %}
    <li>
      <a href="{{ link.url }}"> {{ link.title }}</a>
    </li>
  {% endfor %}
</ol>

{% schema %}
{
  "name": "Header",
  "settings": [
    {
      "type": "link_list",
      "id": "menu",
      "label": "Menu"
    }
  ]
}
{% endschema %}
